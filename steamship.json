{
	"type": "package",
	"handle": "girlfriend-gpt-bot",
	"version": "1.7.0",
	"description": "Katya Vostrikova",
	"author": "Joomipoll",
	"entrypoint": "Unused",
	"public": true,
	"plugin": null,
	"build_config": {
		"ignore": [
			"tests",
			"examples"
		]
	},
	"configTemplate": {
		"bot_token": {
			"type": "string",
			"description": "Telegram bot token",
			"default": "TOKEN"
		},
		"chat_ids": {
			"type": "string",
			"description": "Comma separated list of whitelisted chat_id's",
			"default": " "
		},
		"personality": {
			"type": "string",
			"default": "src/personality/Katya.json"
		},
		"use_gpt4": {
			"type": "boolean",
			"description": "If True, use GPT-4. Use GPT-3.5 if False",
			"default": true
		}
	},
	"steamshipRegistry": {
		"tagline": "",
		"tagline2": null,
		"usefulFor": null,
		"videoUrl": null,
		"githubUrl": null,
		"demoUrl": null,
		"blogUrl": null,
		"jupyterUrl": null,
		"authorGithub": null,
		"authorName": null,
		"authorEmail": null,
		"authorTwitter": null,
		"authorUrl": null,
		"tags": [
			"Telegram",
			"LangChain",
			"GPT"
		]
	}
}
